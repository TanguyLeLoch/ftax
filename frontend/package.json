{
  "name": "ftax-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --configuration development",
    "build-dev": "ng build --output-path=dist",
    "build-prod": "ng build --configuration=production --output-path=dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate model": "openapi-generator-cli generate -i http://localhost:8081/v3/api-docs.yaml -g typescript-angular -o src/app/core/model"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.6",
    "@angular/cdk": "18.2.6",
    "@angular/common": "18.2.6",
    "@angular/compiler": "18.2.6",
    "@angular/core": "18.2.6",
    "@angular/forms": "18.2.6",
    "@angular/material": "18.2.6",
    "@angular/platform-browser": "18.2.6",
    "@angular/platform-browser-dynamic": "18.2.6",
    "@angular/router": "18.2.6",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "chart.js": "^4.4.4",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.6.0",
    "ng2-charts": "^6.0.1",
    "ngx-cookie-service": "^18.0.0",
    "rxjs": "~7.5.0 ",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.6",
    "@angular/cli": "18.2.6",
    "@angular/compiler-cli": "18.2.6",
    "@openapitools/openapi-generator-cli": "^2.10.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.4.0"
  },
  "packageManager": "yarn@4.5.1"
